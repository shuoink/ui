{
  "name": "@shuoink/ui",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "main": "./dist/cms/index.js",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint . --ext=js,jsx,cjs,ts,tsx,mjs",
    "prepare": "husky install && npm run codegen",
    "test": "jest",
    "build": "(rm -rf dist || true) && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc-esm-fix --tsconfig tsconfig.esm.json",
    "codegen": "node --loader ts-node/esm ./scripts/codegen.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "dotenv chromatic",
    "chromatic:ci": "chromatic --exit-zero-on-changes"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@reach/auto-id": "^0.16.0",
    "@spudly/pushpop": "^2.1.3",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-is": "^16.13.1",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.15.0",
    "@shuoink/eslint-config": "^11.0.1",
    "@storybook/addon-essentials": "^6.4.0-beta.31",
    "@storybook/addon-interactions": "^6.4.0-beta.31",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.0-beta.31",
    "@storybook/testing-library": "^0.0.3",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.0",
    "@types/react-is": "^17.0.3",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.0.6",
    "depcheck": "^1.4.2",
    "dotenv-cli": "^4.1.0",
    "eslint": "^7.32.0",
    "expect": "^27.3.1",
    "globby": "^11.0.4",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.4",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^9.0.0",
    "tailwindcss": "^3.0.0-alpha.2",
    "ts-node": "^10.4.0",
    "tsc-esm-fix": "^2.7.4",
    "typescript": "^4.3.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80,
    "quoteProps": "consistent"
  },
  "eslintConfig": {
    "extends": [
      "@shuoink"
    ],
    "plugins": [],
    "rules": {
      "jsx-a11y/label-has-associated-control": "off"
    }
  },
  "eslintIgnore": [
    "coverage",
    "node_modules",
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "private": true
}
