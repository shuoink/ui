    "dist",
    "tailwind-preset.cjs",
    "tailwind-preset.d.ts"
  ],
  "scripts": {
    "build": "(rm -rf dist || true) && tsc -p tsconfig.build.json",
    "dev": "ladle serve",
    "lint": "eslint . --ext=js,jsx,cjs,ts,tsx,mjs,mts,cts",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,htm,html,css}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json",
      "prettier --write"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parserOptions": {
      "extraFileExtensions": [],
      "project": "./tsconfig.json"
    },
    "plugins": [],
    "extends": [
      "@shuoink"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-return": "off",
      "import/extensions": [
        "error",
        "always"
      ],
      "import/no-unresolved": "off",
      "jsdoc/check-examples": "off",
      "node/file-extension-in-import": "off",
      "node/no-missing-import": "off",
      "storybook/no-title-property-in-meta": "off"
    }
  },
  "eslintIgnore": [
    "coverage",
    "node_modules",
    "dist",
    "storybook-static"
  ],
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
