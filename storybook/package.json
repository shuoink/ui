{
  "name": "@shuoink/ui-storybook",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook",
    "chromatic": "dotenv chromatic",
    "chromatic:ci": "chromatic --exit-zero-on-changes",
    "depcheck": "depcheck",
    "lint": "eslint . --ext=js,jsx,cjs,ts,tsx,mjs",
    "storybook": "start-storybook -p 6006"
  },
  "eslintConfig": {
    "plugins": [],
    "extends": [
      "@shuoink"
    ],
    "rules": {
      "jsdoc/no-types": "off",
      "jsdoc/require-description": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "unicorn/filename-case": "off"
    }
  },
  "dependencies": {
    "@shuoink/ui-styles": "workspace:^",
    "@storybook/addons": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@shuoink/eslint-config": "^11.0.1",
    "@storybook/addon-essentials": "^6.4.0-beta.31",
    "@storybook/addon-interactions": "^6.4.0-beta.31",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.0-beta.31",
    "depcheck": "^1.4.2",
    "eslint": "7",
    "postcss": "^8.3.11"
  },
  "peerDependencies": {
    "postcss": "*"
  }
}
